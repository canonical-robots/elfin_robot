  <maintainer email="support@canonicalrobots.com">Support Account (Canonical Robots)</maintainer>
  <license>BSD</license>

  <url type="website">http://ros.org/wiki/elfin_driver</url>
  <url type="bugtracker">https://github.com/canonical-robots/elfin_robot/issues</url>
  <url type="repository">https://github.com/canonical-robots/elfin_robot</url>

  <buildtool_depend>catkin</buildtool_depend>
  <test_depend>rostest</test_depend>
  <depend>industrial_robot_client</depend>
  <depend>control_msgs</depend>

</package>
